{
    "extends": "base_config.json",
    "study_type": "far_field",
    "extraction": {
        "sar": true,
        "power_balance": false,
        "sapd": false,
        "point_sensors": false
    },
    "execution_control": {
        "do_setup": true,
        "only_write_input_file": false,
        "do_run": true,
        "do_extract": true,
        "batch_run": false,
        "auto_cleanup_previous_results": [
            "output"
        ]
    },
    "phantoms": [
        "duke",
        "ella",
        "thelonious",
        "eartha"
    ],
    "frequencies_mhz": [
        26000,
        15000,
        13000,
        11000,
        9000,
        7000
    ],
    "far_field_setup": {
        "type": "environmental",
        "environmental": {
            "incident_directions": [
                "x_neg"
            ],
            "polarizations": [
                "theta"
            ]
        }
    },
    "simulation_parameters": {
        "bbox_padding_mm": 0,
        "simulation_bbox_points": 8,
        "number_of_point_sensors": 0,
        "overall_field_sensor": {
            "enabled": true,
            "record_e_field": true,
            "record_h_field": false,
            "recording_domain": "frequency",
            "on_the_fly_dft": true
        }
    },
    "gridding_parameters": {
        "global_gridding": {
            "grid_mode": "manual",
            "manual_fallback_max_step_mm": 2.0
        },
        "global_gridding_per_frequency": {
            "7000": 0.6,
            "9000": 0.5,
            "11000": 0.5,
            "13000": 0.45,
            "15000": 0.4,
            "26000": 0.3
        },
        "padding": {
            "padding_mode": "manual",
            "manual_bottom_padding_mm": [
                0,
                0,
                0
            ],
            "manual_top_padding_mm": [
                0,
                0,
                0
            ]
        },
        "phantom_bbox_reduction": {
            "use_symmetry_reduction": true,
            "auto_reduce_bbox": false
        }
    }
}
