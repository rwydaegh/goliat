{
    "extends": "far_field_config.json",
    "phantoms": [
        "thelonious"
    ],
    "frequencies_mhz": [
        2450
    ],
    "far_field_setup": {
        "type": "environmental",
        "environmental": {
            "incident_directions": [
                "x_pos",
                "x_neg",
                "y_pos",
                "y_neg"
            ],
            "polarizations": [
                "theta"
            ]
        }
    },
    "execution_control": {
        "do_setup": true,
        "do_run": true,
        "do_extract": false,
        "auto_cleanup_previous_results": []
    },
    "simulation_parameters": {
        "bbox_padding_mm": 30,
        "simulation_time_multiplier": 1.0
    },
    "gridding_parameters": {
        "global_gridding": {
            "grid_mode": "manual",
            "manual_fallback_max_step_mm": 1.4
        },
        "global_gridding_per_frequency": {
            "2450": 1.4
        },
        "padding": {
            "padding_mode": "manual",
            "manual_bottom_padding_mm": [
                0,
                0,
                0
            ],
            "manual_top_padding_mm": [
                0,
                0,
                0
            ]
        },
        "phantom_bbox_reduction": {
            "use_symmetry_reduction": false
        }
    },
    "extract_sapd": false,
    "auto_induced": {
        "enabled": true,
        "top_n": 10,
        "cube_size_mm": 50,
        "save_intermediate_files": true,
        "search": {
            "mode": "air",
            "n_samples": 50000,
            "random_seed": 42,
            "shell_size_mm": 25,
            "selection_percentile": 95.0,
            "min_candidate_distance_mm": 50.0
        }
    }
}