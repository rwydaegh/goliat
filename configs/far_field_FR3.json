{
    "extends": "base_config.json",
    "use_web": false,
    "study_type": "far_field",
    "extraction": {
        "sar": false,
        "power_balance": false,
        "sapd": false,
        "point_sensors": false
    },
    "execution_control": {
        "do_setup": true,
        "only_write_input_file": false,
        "do_run": true,
        "do_extract": false,
        "batch_run": false,
        "auto_cleanup_previous_results": []
    },
    "phantoms": [
        "duke",
        "ella",
        "eartha",
        "thelonious"
    ],
    "frequencies_mhz": [
        15000,
        13000,
        11000,
        9000,
        7000
    ],
    "auto_induced": {
        "enabled": true,
        "top_n": 10,
        "cube_size_mm": 50,
        "search_metric": "E_magnitude",
        "save_intermediate_files": false,
        "use_xy_diagonal_for_sim_time": true,
        "search": {
            "mode": "air",
            "n_samples": 10000,
            "random_seed": 42,
            "shell_size_mm": 10.0,
            "selection_percentile": 95.0,
            "min_candidate_distance_mm": 50.0
        }
    },
    "far_field_setup": {
        "type": "environmental",
        "environmental": {
            "spherical_tessellation": {
                "theta_values": [
                    90
                ],
                "phi_values": [
                    90,
                    150,
                    210,
                    270
                ]
            },
            "polarizations": [
                "theta"
            ]
        },
        "auto_induced": {
            "grid_resolution_deg": 15
        }
    },
    "simulation_parameters": {
        "bbox_padding_mm": 0,
        "simulation_bbox_points": 8,
        "number_of_point_sensors": 0,
        "simulation_time_multiplier": 3.0
    },
    "gridding_parameters": {
        "global_gridding": {
            "grid_mode": "manual",
            "manual_fallback_max_step_mm": 3.0
        },
        "global_gridding_per_frequency": {
            "450": 2.5,
            "700": 2.5,
            "835": 2.5,
            "1450": 2.5,
            "2140": 1.694,
            "2450": 1.482,
            "3500": 1.0,
            "5200": 1.0,
            "5800": 1.0,
            "7000": 0.6,
            "9000": 0.5,
            "11000": 0.5,
            "13000": 0.45,
            "15000": 0.4
        },
        "padding": {
            "padding_mode": "manual",
            "manual_bottom_padding_mm": [
                0,
                0,
                0
            ],
            "manual_top_padding_mm": [
                0,
                0,
                0
            ]
        },
        "phantom_bbox_reduction": {
            "auto_reduce_bbox": true,
            "reference_frequency_mhz": 7000,
            "use_symmetry_reduction": true,
            "height_limit_per_frequency_mm": {}
        }
    }
}
